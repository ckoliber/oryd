<fieldset>
    <label>
        <input
            name="<%= it.ui.attributes.name %>"
            type="<%= it.ui.attributes.type %>"
            value="<%= it.ui.attributes.value %>"
            disabled="<%= it.ui.attributes.disabled %>"
            placeholder="<%= it.t(it.ui.meta.label) %>"
        />
        <span><%= it.t(it.ui.meta.label) %></span>
    </label>
    <%= JSON.stringify(it.ui.messages) %>
</fieldset>
